  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saketkumar1996/MERN-Stack-E-commerce-application/issues"
  },
  "homepage": "https://github.com/saketkumar1996/MERN-Stack-E-commerce-application#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "color": "^4.2.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.7.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
